@using ERP.ResourcePack.AidModule
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="table-responsive">
            <table class="table table-bordered table-striped table-hover" id="tblDuties" style="width:100%">
                <thead>
                    <tr>
                        <th><i class="fas fa-hashtag"></i></th>
                        <th>@ScPortalRequests.DutyType</th>
                        <th>@ScPortalRequests.DutyDescription</th>
                        <th>@ScPortalRequests.MonthlyAmount</th>
                        <th>@ScPortalRequests.Total</th>
                        <th><i class="fas fa-cog"></i></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
