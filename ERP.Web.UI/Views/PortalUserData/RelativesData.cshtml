@using ERP.ResourcePack.AidModule
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="table-responsive">
            <table class="table table-bordered table-striped table-hover" id="tblFndUserRelatives" style="width:100%">
                <thead>
                    <tr>
                        <th><i class="fas fa-hashtag"></i></th>
                        <th>@FndUsers.Name</th>
                        <th>@FndUsers.GenderLkpId</th>
                        <th>@FndUsers.RelativesTypeLkpId</th>
                        <th>@FndUsers.NationalityLkpId</th>
                        <th>@FndUsers.IdNumber</th>
                        <th>@FndUsers.QualificationLkpId</th>
                        <th>@FndUsers.MaritalStatusLkpId</th>
                        <th>@FndUsers.Employer</th>
                        <th>@FndUsers.Notes</th>
                        <th><i class="fas fa-cog"></i></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
