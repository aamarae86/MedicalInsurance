                                <th>@ApBanks.BankAccountNameEn</th>
                                <th>@ApBanks.CurrencyId</th>
                                <th>@ApBanks.CurrencyRate</th>
                                <th><i class="fas fa-money-check-alt"></i></th>
                                <th>@ApBanks.IsActive</th>
                                <th><i class="fas fa-cogs"></i></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
