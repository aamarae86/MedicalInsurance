@using ERP.ResourcePack.AccountsExtend
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="table-responsive">
            <table class="table table-bordered table-striped table-hover" id="tblFaAssetDeprnTr" style="width:100%">
                <thead>
                    <tr>
                        <th><i class="fas fa-hashtag"></i></th>
                        <th>@FaAsset.AssetNumber</th>
                        <th>@FaAsset.Desc</th>
                        <th>@FaAsset.OriginalCost</th>
                        <th>@FaAsset.CurrentCost</th>
                        <th>@FaAsset.ProrateDate</th>
                        <th>@FaAsset.SalvageValue</th>
                        <th>@FaAsset.LifeInMonths</th>
