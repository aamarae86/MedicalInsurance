@using ERP.Web.UI.Controllers
@using ERP.ResourcePack.Accounts

<div class="row no-gutters clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header d-flex justify-content-start align-items-center">
                <i class="fas fa-2x fa-chevron-down down-arr mr-2" data-toggle="collapse" data-target="#dataCollapse"></i>
                <h2> <strong>@ReportsAccounts.TitleTenant</strong> 
                @*-  @ERP.ResourcePack.Common.Settings.Data*@ </h2>
            </div>
            <div class="body collapse show" id="dataCollapse">
                <div class="table-responsive">
                    <div id="tblLoader" style="display:none">
                        <div class="lds-roller"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
                    </div>
                    <table class="dataTable dt-responsive no-footer nowrap table table-bordered table-striped w-100" id="tblTenantFreeModules">
                        <thead>
                            <tr>
                                <th><i class="fas fa-hashtag"></i></th>
                                <th>@ERP.ResourcePack.Common.Settings.UsrName</th>
                                <th>@ReportsAccounts.FullName</th>
                                <th>@ReportsAccounts.TenantNameAr</th>
                                <th>@ReportsAccounts.Tel</th>
                                <th>@ReportsAccounts.AdminValue</th>
                                <th>@ReportsAccounts.UserValue</th>
                                <th>@ReportsAccounts.CreationTime</th>
                                <th>@ERP.ResourcePack.Common.Settings.EndDate</th>
                                <th>@ReportsAccounts.RemainingDays</th>
                                <th>@ReportsAccounts.Email</th>
                                <th>@ReportsAccounts.MobileNo</th>
                                <th class="all"><i class="fas fa-cogs"></i></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>