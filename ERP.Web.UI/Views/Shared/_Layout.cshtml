    <script>

        (function ($) {

            $('[data-toggle="tooltip"]').tooltip();



            //Time Ago Plugin Arabic Setup
            if ($('html').attr('lang') == "ar-EG") {
                if (typeof ($.timeago) != "undefined") {
                    jQuery.timeago.settings.strings = {
                        prefixAgo: null,
                        prefixFromNow: null,
                        suffixAgo: null,
                        suffixFromNow: "منذ الأن",
                        seconds: "منذ أقل من دقيقة",
                        minute: "منذ دقيقة",
                        minutes: "منذ %d دقائق",
                        hour: "منذ ساعة",
                        hours: "منذ %d ساعات",
                        day: "منذ يوم",
                        days: "منذ %d أيام",
                        month: "منذ شهر",
                        months: "منذ %d شهر",
                        year: "منذ سنة",
                        years: "%d سنين",
                        wordSeparator: " ",
                        numbers: []
                    };
                };
            }





            //serializeFormToObject plugin for jQuery
            $.fn.serializeFormToObject = function () {
                //serialize to array
                var data = $(this).serializeArray();

                //add also disabled items
                $(':disabled[name]', this).each(function () {
                    data.push({ name: this.name, value: $(this).val() });
                });

                //map to object
                var obj = {};
                data.map(function (x) { obj[x.name] = x.value; });

                return obj;
            };

            //Configure blockUI
            if ($.blockUI) {
                $.blockUI.defaults.baseZ = 2000;
            }

        })(jQuery);



    </script>

