<script>

    $(function () {

        $('#SCCasesListReport').addClass('active');
    });
    $('#NationalityLkpId').select2(ReturnSelect2ObjectForLkpsTypes('Nationality'));
    $('#CityLkpId').select2(ReturnSelect2ObjectForLkpsTypes('City'));

    const PrintTmCharityBoxListReport = function () {
         
       
        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsAids/PrintSCCasesList`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:@Session["TenantId"],
                NationalityLkpId: $('#NationalityLkpId').find(':selected').val(),
                NationalityLkpIdtxt: $('#NationalityLkpId').find(':selected').text(),
                CityLkpId: $('#CityLkpId').find(':selected').val(),
                CityLkpIdtxt: $('#CityLkpId').find(':selected').text(),
                IdNumber: $('#IdNumber').val() ? $('#IdNumber').val().split('-').join('') : '',
                IsZakat: ($('#chkIsZakat').is(":checked")) ? true : false
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };


</script>