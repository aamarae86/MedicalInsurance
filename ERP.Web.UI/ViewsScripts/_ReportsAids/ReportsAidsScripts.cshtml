<script>

    $(function () {

        $('#ReportsAids').addClass('active');
    });
    $('#StatusLKkpId').select2(ReturnSelect2ObjectForLkpsTypes('Nationality'));

    const PrintTmCharityBoxListReport = function () {

        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsAids/PrintReportsAids`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:@Session["TenantId"],
                NationalityLkpId: $('#StatusLKkpId').find(':selected').val(),
                NationalityLkpIdtxt: $('#StatusLKkpId').find(':selected').text(),
                FromDate: $('#FromDate').val(),
                ToDate: $('#ToDate').val()
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };


</script>