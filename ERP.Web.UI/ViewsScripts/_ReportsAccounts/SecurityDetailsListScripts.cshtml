<script>
    
    
    $(function () {
        $('#SecurityDetailsList').addClass('active');
    });

    const PrintAccountStatementReport = function () {

        

        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsAccounts/PrintSecurityDetailsList`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:0,
                ArCustomerId: $('#ArCustomerId').find(':selected').val(),
                ArCustomerIdtxt:  $('#ArCustomerId').find(':selected').text(),
                StatusLkpId: $('#StatusLkpId').find(':selected').val(),
                StatusLkpIdtxt:  $('#StatusLkpId').find(':selected').text()
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };
    $('#ArCustomerId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/ArCustomers/GetArCustomersSelect2`));
    $('#StatusLkpId').select2(ReturnSelect2ObjectForLkpsTypes('ArSecurityDepositInterfacePost'));

</script>