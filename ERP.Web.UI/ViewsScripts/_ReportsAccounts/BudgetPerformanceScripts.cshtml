<script>
    
    
    $(function () {
        $('#BudgetPerformance').addClass('active');
    });

    const PrintAccountStatementReport = function () {

        if (!$('#FromPeriod').val() || !$('#GlAccMappingHd').val() || !$('#ToPeriod').val()) {
            toastr.warning('@ERP.ResourcePack.Accounts.ReportsAccounts.EnterData');
            return;
        }

        const CodeComReport = GetCodeComUtilitesForReports(__arrGlSelect2, 'AccId');
     
        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsAccounts/PrintBudgetPerformance`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:0,
                Attribute1: CodeComReport.attr1,
                Attribute2: CodeComReport.attr2,
                Attribute3: CodeComReport.attr3,
                Attribute4: CodeComReport.attr4,
                Attribute5: CodeComReport.attr5,
                Attribute6: CodeComReport.attr6,
                Attribute7: CodeComReport.attr7,
                Attribute8: CodeComReport.attr8,
                Attribute9: CodeComReport.attr9,
                AccId: CodeComReport.accId,
                PeriodIdFrom: $('#FromPeriod').find(':selected').val(),
                PeriodIdFromtxt:  $('#FromPeriod').find(':selected').text(),
                GlAccMappingHdId: $('#GlAccMappingHd').find(':selected').val(),
                GlAccMappingHdIdtxt:  $('#GlAccMappingHd').find(':selected').text(),
                PeriodIdTo: $('#ToPeriod').find(':selected').val() ,
                PeriodIdTotxt: $('#ToPeriod').find(':selected').text() 
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };
    $('#FromPeriod').select2(ReturnSelect2ObjectDefault(`${baseUrl}/GlPeriodsDetails/GetGlPeriodsSelect2`));

    $('#ToPeriod').select2(ReturnSelect2ObjectDefault(`${baseUrl}/GlPeriodsDetails/GetGlPeriodsSelect2`));

    $('#GlAccMappingHd').select2(ReturnSelect2ObjectDefault(`${baseUrl}/GlAccMappingHd/GetGlAccMappingHdSelect2`));

</script>