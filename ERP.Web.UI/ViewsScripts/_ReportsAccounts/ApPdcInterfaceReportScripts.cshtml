<script>

    $(function () {
        $('#ApPdcInterfaceReport').addClass('active');
        $('#StatusLkpId').select2(ReturnSelect2ObjectForLkpsTypes('@ERP.Front.Helpers.Enums.Common.FndLkpsTypes.ApPdcInterfaceStatues.ToString()'));
        $('#BankAccountId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/ApBankAccounts/GetApBankAccountsSelect2`));

    });

    const PrintApPdcInterfaceReport = function () {

        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsAccounts/PrintApPdcInterfaceReport`,
            method: 'POST',
            header: {
                'Authorization': `bearer ${$_token}`
            },
            data: {
                fromDate: $('#fromDate').val(),
                toDate: $('#toDate').val(),
                statusLkpId: $('#StatusLkpId').find(':selected').val(),
                statusLkpIdtxt: $('#StatusLkpId').find(':selected').text(),
                bankAccountId: $('#BankAccountId').find(':selected').val()
               
            }
        }).done(function (data) {

            if (data.length > 0) {

                $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

                $('#bodyss').html('')
                $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
                $('#model').modal('show');

            }
            else {
                $('.page-loader-wrapper').css('display', 'none');
                toastr.warning(NoData);
            }

        }).fail(function (response, x, r) {
                $('.page-loader-wrapper').css('display', 'none');
            toastr.error(response.responseText);
        });
    };

</script>