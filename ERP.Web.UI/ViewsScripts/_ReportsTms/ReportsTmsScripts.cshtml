<script>
    $(function () {
        $('#TmCharityBoxList').addClass('active');
    });
    $('#CharityStatus').select2(ReturnSelect2ObjectForLkpsTypes('TmCharityBoxesStatus'));
    $('#CityId').select2(ReturnSelect2ObjectForLkpsTypes('City'));
    $('#CharityId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/TmCharityBoxes/GetSelect2`));
    $('#BoxTypeName').select2(ReturnSelect2ObjectDefault(`${baseUrl}/TmCharityBoxesType/GetSelect2`));
    $('#RegionId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/TmRegions/GetTmRegionsSelect2`));
    $('#LocationId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/TmLocations/GetTmLocationsSelect2`));
    $('#LocationSubId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/TmLocations/GetTmLocationSubSelect2`));

    const PrintTmCharityBoxListReport = function () {

        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsTms/PrintTmCharityBoxList`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:@Session["TenantId"],
                CharityId: $('#CharityId').val(),
                CharityIdtxt: $('#CharityId').text(),
                CharityBoxBarcode:$('#CharityBoxBarcode').val(),
                CharityStatus:$('#CharityStatus').val(),
                CharityStatustxt:$('#CharityStatus').text(),
                BoxTypeName: $('#BoxTypeName').val(),
                BoxTypeNametxt: $('#BoxTypeName').text(),
                CityId: $('#CityId').val(),
                CityIdtxt: $('#CityId').text(),
                RegionId: $('#RegionId').val(),
                RegionIdtxt: $('#RegionId').text(),
                LocationId: $('#LocationId').val(),
                LocationIdtxt: $('#LocationId').text(),
                LocationSubId: $('#LocationSubId').val(),
                LocationSubIdtxt: $('#LocationSubId').text()
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };


</script>