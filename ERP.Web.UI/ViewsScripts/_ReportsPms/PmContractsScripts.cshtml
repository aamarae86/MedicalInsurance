<script>
    $(function () {

        $('#PmContracts').addClass('active');
    });

    $('#StatusLKkpId').select2(ReturnSelect2ObjectForLkpsTypes('PmContractPost'));
    $('#PmActivityId').select2(ReturnSelect2ObjectForLkpsTypes('PmOwnersTaxDetailsType'));
    $('#PmUnitTypeLkpId').select2(ReturnSelect2ObjectForLkpsTypes('PmContractUnitType'));
    $('#PropertyId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/PmProperties/GetPmPropertiesSelect2`));
    $('#PMTenantId').select2(ReturnSelect2ObjectDefault(`${baseUrl}/PmTenants/GetPmTenantsSelect2`));


    const PrintTmCharityBoxListReport = function () {

        $('.page-loader-wrapper').css('display', 'block');

        $.ajax({
            url: `${$PrefixHostName}/ReportsPms/PrintPmContracts`,
            method: 'POST',
            header: {
                'Authorization':`bearer ${$_token}`
            },
            data: {
                Lang: lang,
                TenantId:@Session["TenantId"],
                PmActivityId: $('#PmActivityId').val(),
                PmActivityIdtxt: $('#PmActivityId').text(),
                PMTenantId: $('#PMTenantId').val(),
                PMTenantIdtxt: $('#PMTenantId').text(),
                StatusLKkpId: $('#StatusLKkpId').val(),
                StatusLKkpIdtxt: $('#StatusLKkpId').text(),
                PmUnitTypeLkpId:$('#PmUnitTypeLkpId').val(),
                PmUnitTypeLkpIdtxt:$('#PmUnitTypeLkpId').text(),
                FromDate: $('#FromDate').val(),
                ToDate: $('#ToDate').val(),
                ContractStartDate: $('#ContractStartDate').val(),
                contractEndDate: $('#contractEndDate').val(),
                PropertyId: $('#PropertyId').val(),
                PropertyIdtxt: $('#PropertyId').text()
            }
        }).done(function (data) {

            if (data.length > 0) {
                           
              $('.page-loader-wrapper').css('display', 'none');
                var src = formAction;

              $('#bodyss').html('')
              $('#bodyss').append('<iframe id="ifram1" style="width:100%;height:550px" src="' + src + '"></iframe>')
              $('#model').modal('show');

            }
            else {
                  $('.page-loader-wrapper').css('display', 'none');
                  toastr.warning(NoData);
            }

        }).fail(function (response,x,r) {
            toastr.error(response.responseText);
        });

    };


</script>